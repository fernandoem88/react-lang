{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "type": "module",
  "module": "dist/react-lang.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "rollup -c",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "rollup -c",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-lang",
  "author": "Fernando Ekutsu Mondele",
  "size-limit": [
    {
      "path": "dist/react-lang.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-lang.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.1.6",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "husky": "^9.1.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.29.1",
    "rollup-plugin-typescript2": "^0.36.0",
    "size-limit": "^11.1.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "types": "dist/index.d.ts"
  
}
